{
  "name": "mtg-backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --exec babel-node -r dotenv/config ./src/index.js",
    "start-prod": "pm2 start ./dist/index.js --name backend --interpreter ./node_modules/.bin/babel-node --node-args=\"-r dotenv/config\"",
    "stop-prod": "pm2 delete backend",
    "build": "babel src --out-dir dist",
    "create-migration": "babel-node -r dotenv/config node_modules/.bin/knex migrate:make new_migration",
    "migrate": "babel-node -r dotenv/config node_modules/.bin/knex migrate:latest",
    "rollback": "babel-node -r dotenv/config node_modules/.bin/knex migrate:rollback",
    "update-cards": "babel-node -r dotenv/config src/scripts/updateCardsManually.js",
    "update-card-images": "babel-node -r dotenv/config src/scripts/updateAllCardImages.js",
    "update-sets": "babel-node -r dotenv/config src/scripts/updateSetsManually.js",
    "backup-db": "babel-node -r dotenv/config src/scripts/backupDBManually.js",
    "store-prices": "babel-node -r dotenv/config src/scripts/storeCardProceManually.js",
    "lint": "eslint --fix src"
  },
  "dependencies": {
    "@getvim/execute": "^1.0.0",
    "apollo-server-koa": "^3.12.0",
    "body-parser": "^1.20.2",
    "cron": "^2.3.0",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "google-auth-library": "^8.7.0",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "graphql-tools": "^8.3.20",
    "http-status-codes": "^2.2.0",
    "knex": "^2.4.2",
    "koa": "^2.14.2",
    "koa-session": "^6.4.0",
    "line-by-line": "^0.1.6",
    "lodash": "^4.17.21",
    "merge-graphql-schemas": "^1.5.8",
    "mysql": "^2.17.1",
    "node-fetch": "2.6.9",
    "pg": "^8.10.0",
    "sharp": "^0.32.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/node": "7.20.7",
    "@babel/plugin-proposal-numeric-separator": "^7.18.6",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.21.4",
    "eslint": "^8.38.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.22",
    "pm2": "^5.3.0"
  }
}
